@{
}
<html>
    <head>
        <script>
            $(document).ready(function () {
                var options = {};
                options.url = "/Values/currencies";
                options.Type = "GET";
                options.dataType = "json";
                options.success = function (Data) {
                    Data.forEach(function (element) {
                        var row = "<tr>";
                        row += "<td>";
                        row += element.NumCode;
                        row += "</td>";
                        row += "<td>";
                        row += element.Nominal;
                        row += "</td>";
                        row += "<td>";
                        row += element.Name;
                        row += "</td>";
                        row += "<td>";
                        row += element.Value;
                        row += "</td>";
                        row += "<td>";
                        row += element.Previous;
                        row += "</td>";
                        row += "</tr>";
                        $("#ResultTable").append(row);
                    });
                };
                $.ajax(options);
            });


        </script>
    </head>
    <body>
        <button onclick=""></button>
        <table id="ResultTable" class="table">
            <tr>
                <th>NumCode</th>
                <th>Nominal</th>
                <th>Name</th>
                <th>Value</th>
                <th>Previous</th>
            </tr>
        </table>
    </body>
</html>